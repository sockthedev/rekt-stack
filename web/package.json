{
  "name": "@rekt/web",
  "version": "0.0.0",
  "private": true,
  "sideEffects": false,
  "scripts": {
    "build": "run-s \"build:*\"",
    "build:css": "tailwindcss -m -i ./style/tailwind.css -o ./app/style.css",
    "build:remix": "remix build",
    "dev": "run-p \"dev:*\"",
    "dev:css": "tailwindcss -w -i ./style/tailwind.css -o ./app/style.css",
    "dev:remix": "sst-env -- remix dev"
  },
  "dependencies": {
    "@rekt/services": "0.0.0",
    "@remix-run/node": "^1.7.2",
    "@remix-run/react": "^1.7.2",
    "@remix-run/serve": "^1.7.2",
    "@tanstack/react-query": "^4.3.9",
    "@trpc/client": "^10.0.0-proxy-beta.5",
    "@trpc/react": "^10.0.0-proxy-beta.5",
    "@trpc/server": "^10.0.0-proxy-beta.5",
    "clsx": "^1.2.1",
    "node-fetch": "2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tiny-invariant": "^1.2.0"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.7.2",
    "@remix-run/eslint-config": "^1.7.2",
    "@serverless-stack/static-site-env": "^1.15.0",
    "@types/node-fetch": "^2.6.2",
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "autoprefixer": "^10.4.8",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.16",
    "tailwindcss": "^3.1.8",
    "typescript": "^4.7.4"
  }
}
